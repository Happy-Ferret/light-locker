#: ../src/light-locker-command.c:40
msgid "Tells the running display manager to lock the screen immediately"
msgstr "Powoduje natychmiastowe zablokowanie ekranu w uruchomionym menedżerze ekranu"

#: ../src/light-locker-command.c:42 ../src/light-locker.c:56
#: ../src/preview.c:70
msgid "Version of this application"
msgstr "Wersja tego programu"

#: ../src/light-locker.c:57 ../src/preview.c:71
msgid "Enable debugging code"
msgstr "Włącza kod debugowania"

#: ../src/light-locker.c:59
msgid "Lock the screen S seconds after the screensaver started"
msgstr "Zablokuj ekran S sekund po uruchomieniu wygaszacza ekranu"

#: ../src/light-locker.desktop.in.in.h:1
msgid "Screen Locker"
msgstr "Blokada ekranu"

#: ../src/light-locker.desktop.in.in.h:2
msgid "Launch screen locker program"
msgstr "Uruchom program blokady ekranu"

#: ../src/gs-content.c:96
msgid "This session is locked"
msgstr "Ta sesja jest zablokowana"

#: ../src/gs-content.c:118
msgid ""
"You'll be redirected to the unlock dialog automatically in a few seconds"
msgstr "W przeciągu kilku sekund zostaniesz automatycznie przekierowany do okna odblokowania"

#: ../src/preview.c:72
msgid "Preview screen width"
msgstr "Szerokość podglądu ekranu"

#: ../src/preview.c:73
msgid "Preview screen height"
msgstr "Wysokość podglądu ekranu"
